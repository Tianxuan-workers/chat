#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/../../../../../@unocss+cli@0.50.6/node_modules/@unocss"
else
  export NODE_PATH="$NODE_PATH:$basedir/../../../../../@unocss+cli@0.50.6/node_modules/@unocss"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@unocss+cli@0.50.6/node_modules/@unocss/cli/bin/unocss.mjs" "$@"
else
  exec node  "$basedir/../../../../../@unocss+cli@0.50.6/node_modules/@unocss/cli/bin/unocss.mjs" "$@"
fi
